repeat wait() until game:IsLoaded()
pcall(function()
for i, v in pairs(game:GetService("Workspace").Terrain["_Game"].Admin:GetChildren()) do
    if v.Name == "Regen" then
        	if not game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild(game.Players.LocalPlayer.Name .. "'s admin") then
			if game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin") then
				local pad = game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin"):FindFirstChild("Head")
				local padCFrame = game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin"):FindFirstChild("Head").CFrame
				wait(0.125)
				pad.CanCollide = false
				repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
				pad.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				wait(0.125)
				pad.CFrame = padCFrame
				pad.CanCollide = true
			else
				fireclickdetector(game:GetService("Workspace").Terrain["_Game"].Admin.Regen.ClickDetector, 0)
			end
		end
		wait(0.5)
function start(plr)
    plr.Chatted:Connect(function(msg)
        if string.sub(msg,1,4) == "logs" or string.sub(msg,1,5) == ":logs" then
                    for i=1,250 do
                        game.Players:Chat("h "..plr.."You cannot use logs.")
                        end
            end
        end)
    end
		
		function punish(x)
    game.Players:Chat("punish "..x)
end

wait(1) do
for i, v in pairs(game.Players:GetChildren()) do
    if v.Name == game.Players.LocalPlayer.Name then
        else
game.Players:Chat("respawn "..v.Name)
for i=1, 5 do
    wait(.02)
pcall(punish, v.Name)
end
pcall(start, v.Name)
end
end
end

					fireclickdetector(game:GetService("Workspace").Terrain["_Game"].Admin.Regen.ClickDetector, 0)
					wait(1)
		if not game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild(game.Players.LocalPlayer.Name .. "'s admin") then
			if game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin") then
				local pad = game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin"):FindFirstChild("Head")
				local padCFrame = game:GetService("Workspace").Terrain["_Game"].Admin.Pads:FindFirstChild("Touch to get admin"):FindFirstChild("Head").CFrame
				wait(0.125)
				pad.CanCollide = false
				repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
				pad.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				wait(0.125)
				pad.CFrame = padCFrame
				pad.CanCollide = true
			else
				fireclickdetector(game:GetService("Workspace").Terrain["_Game"].Admin.Regen.ClickDetector, 0)
			end
		end
		
game.Players.PlayerAdded:Connect(function(plr)
    wait(1)
    for i=1,10 do
        wait(0.3)
game.Players:Chat("respawn "..plr.Name)
pcall(punish, plr.Name)
end
pcall(start, plr.Name)
end)
end
end
local lclplrname = game.Players.LocalPlayer
lclplrname.Chatted:Connect(function(mesg)
    if string.sub(mesg,1,7) == ":punish" then
        for i, v in pairs(game.Players:GetChildren()) do
    if v.Name == game.Players.LocalPlayer.Name then
        else
game.Players:Chat("respawn "..v.Name)
for i=1, 5 do
    wait(.02)
pcall(punish, v.Name)
end
pcall(start, v.Name)
end
end
elseif string.sub(mesg,1,6) == ":regen" then
    	fireclickdetector(game:GetService("Workspace").Terrain["_Game"].Admin.Regen.ClickDetector, 0)
elseif string.sub(mesg,1,7) == ":rejoin" then
	    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId)
	    elseif string.sub(mesg,1,4) == ":hop" then
	     local x = {}
        for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
            if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
                x[#x + 1] = v.id
            end
        end
        if #x > 0 then
            game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, x[math.random(1, #x)])
        else
                        game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, x[math.random(1, #x)])
end
end
    end)

end)
