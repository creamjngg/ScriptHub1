        pcall(function()
         for _,plr in pairs(game:GetService('Players'):GetChildren()) do
             if	plr.Character then
			if plr.Character.Parent.Name == 'Players' then
				if not plr.Character.Head:FindFirstChild('OriginalSize') then
					plr.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ðŸ’¸]' .. plr.DisplayName)
				end
			end
             end
         end
	end)
